#include<bits/stdc++.h>
using namespace std;

int main() {
#ifndef ONLINE_JUDGE
	//get input from input1.txt
	freopen("input1.txt", "r", stdin);
	//write output in output1.txt
	freopen("output1.txt", "w", stdout);
#endif
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		if (n <= 30) {
			cout << "NO" << endl;
		}
		else {
			if (n - 30 == 14) {
				cout << "YES" << endl ;
				cout << n - 31 << " " << 15 << " " << 10 << " " << 6 << endl;
			}
			else if (n - 30 == 10) {
				cout << "YES" << endl ;
				cout << n - 35 << " " << 15 << " " << 14 << " " << 6 << endl;
			}
			else if (n - 30 == 6) {
				cout << "YES" << endl ;
				cout << n - 31 << " " << 15 << " " << 10 << " " << 6 << endl;

			}
			else {
				cout << "YES" << endl ;
				cout << n - 30 << " " << 14 << " " << 10 << " " << 6 << endl;
			}
		}
	}
	return 0;
}
